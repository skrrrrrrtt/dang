local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Wait for GUI to load
wait(2)

-- Function to get rank quests from GUI
local function showRankQuests()
    local playerGui = player:WaitForChild("PlayerGui")
    local instanceQuests = playerGui:WaitForChild("InstanceQuests")
    local questsHolder = instanceQuests.Frame.Quests.QuestsGradient.QuestsHolder
    
    print("\n=== YOUR RANK QUESTS ===\n")
    
    -- Loop through all quest elements
    for _, questFrame in pairs(questsHolder:GetChildren()) do
        if questFrame.Name == "Quest" and questFrame:IsA("Frame") then
            -- Get quest title
            local title = questFrame:FindFirstChild("Title")
            if title then
                print("Quest: " .. title.Text)
            end
            
            -- Get progress
            local progress = questFrame:FindFirstChild("Progress")
            if progress then
                print("Progress: " .. progress.Text)
            end
            
            print("---")
        end
    end
    
    print("========================\n")
end

-- Show quests
showRankQuests()

-- Make it callable
_G.showRankQuests = showRankQuests

print("âœ… Quest reader loaded!")
print("Type '_G.showRankQuests()' to check again")
