-- Function to update GUI
local function updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
    if config.autoLockpick then
        statusLabel.Text = "Status: ON"
        statusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
    else
        statusLabel.Text = "Status: OFF"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
    
    local displayName = config.lockpickType == "Lockpick A" and "Normal üîë" 
                     or config.lockpickType == "Lockpick B" and "Golden üü°" 
                     or "Diamond üíé"
    typeLabel.Text = "Lockpick: " .. displayName
    
    local doorDifficulty = config.doorNumber == 1 and "(Easy)" or config.doorNumber == 2 and "(Medium)" or "(Hard)"
    doorLabel.Text = "Door: " .. config.doorNumber .. " " .. doorDifficulty .. " üö™"
    
    -- Update lockpick button colors
    normalButton.BackgroundColor3 = config.lockpickType == "Lockpick A" and Color3.fromRGB(100, 150, 255) or Color3.fromRGB(80, 80, 90)
    goldenButton.BackgroundColor3 = config.lockpickType == "Lockpick B" and Color3.fromRGB(255, 215, 0) or Color3.fromRGB(80, 80, 90)
    diamondButton.BackgroundColor3 = config.lockpickType == "Lockpick C" and Color3.fromRGB(100, 200, 255) or Color3.fromRGB(80, 80, 90)
    
    -- Update door button colors
    door1Button.BackgroundColor3 = config.doorNumber == 1 and Color3.fromRGB(100, 200, 100) or Color3.fromRGB(80, 80, 90)
    door2Button.BackgroundColor3 = config.doorNumber == 2 and Color3.fromRGB(255, 180, 0) or Color3.fromRGB(80, 80, 90)
    door3Button.BackgroundColor3 = config.doorNumber == 3 and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(80, 80, 90)
end

-- Function to attempt lockpick
local function attemptLockpick()
    print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
    print("üö™ Door:", config.doorNumber, "| Lockpick:", config.lockpickType)
    print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
    
    -- Step 1: Request Unlock with lockpick type AND door number
    local requestUnlock = Network:FindFirstChild("Lockpick Game: Request Unlock")
    if requestUnlock then
        local success, response = pcall(function()
            -- Try with door number parameter
            return requestUnlock:InvokeServer(config.lockpickType, config.doorNumber)
        end)
        
        if success and response then
            print("‚úÖ Lockpick game started!")
        else
            -- If that didn't work, try without door number
            success, response = pcall(function()
                return requestUnlock:InvokeServer(config.lockpickType)
            end)
            
            if not success or not response then
                warn("‚ö†Ô∏è Failed to start lockpick. Make sure you're near a Grinch Cove door!")
                warn("‚ö†Ô∏è Error:", response)
                return false
            end
        end
    else
        warn("‚ö†Ô∏è 'Lockpick Game: Request Unlock' not found!")
        return false
    end
    
    wait(0.3)
    
    -- Step 2: Perform clicks (the minigame requires clicks)
    local clickEvent = Network:FindFirstChild("Click")
    if clickEvent then
        for i = 1, config.clicksPerAttempt do
            pcall(function()
                clickEvent:FireServer()
            end)
            wait(0.08)
        end
        print("üñ±Ô∏è Performed", config.clicksPerAttempt, "clicks")
    end
    
    wait(0.5)
    
    -- Step 3: Claim the result
    local claimEvent = Network:FindFirstChild("Lockpick Game: Claim")
    if claimEvent then
        local success, result = pcall(function()
            return claimEvent:InvokeServer()
        end)
        
        if success and result then
            print("üéâ SUCCESS! Door", config.doorNumber, "unlocked!")
            return true
        else
            print("‚ùå Lockpick broke! Retrying...")
            return false
        end
    end
    
    return false
end

-- Create the GUI
local gui, toggleButton, statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button = createGUI()

-- Main lockpick loop
spawn(function()
    while wait(0.5) do
        if config.autoLockpick then
            local success, err = pcall(attemptLockpick)
            
            if not success then
                warn("‚ö†Ô∏è Error:", err)
            end
            
            wait(config.delayBetweenAttempts)
        end
    end
end)

-- Toggle button
toggleButton.MouseButton1Click:Connect(function()
    config.autoLockpick = not config.autoLockpick
    
    if config.autoLockpick then
        toggleButton.BackgroundColor3 = Color3.fromRGB(50, 220, 50)
        toggleButton.Text = "STOP"
        print("üü¢ Auto Lockpick STARTED!")
    else
        toggleButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        toggleButton.Text = "START"
        print("üî¥ Auto Lockpick STOPPED!")
    end
    
    updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
end)

-- Lockpick selection buttons
normalButton.MouseButton1Click:Connect(function()
    config.lockpickType = "Lockpick A"
    print("üîë Selected: Normal Lockpick")
    updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
end)

goldenButton.MouseButton1Click:Connect(function()
    config.lockpickType = "Lockpick B"
    print("üü° Selected: Golden Lockpick")
    updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
end)

diamondButton.MouseButton1Click:Connect(function()
    config.lockpickType = "Lockpick C"
    print("üíé Selected: Diamond Lockpick")
    updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
end)

-- Door selection buttons
door1Button.MouseButton1Click:Connect(function()
    config.doorNumber = 1
    print("üö™ Selected: Door 1 (Easy)")
    updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
end)

door2Button.MouseButton1Click:Connect(function()
    config.doorNumber = 2
    print("üö™ Selected: Door 2 (Medium)")
    updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
end)

door3Button.MouseButton1Click:Connect(function()
    config.doorNumber = 3
    print("üö™ Selected: Door 3 (Hard)")
    updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
end)

print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
print("üéÆ PS99 Auto Lockpick Loaded!")
print("‚ö†Ô∏è IMPORTANT:")
print("1. Stand CLOSE to a Grinch Cove door")
print("2. Have lockpicks in your inventory")
print("3. Select door number & lockpick type")
print("4. Press START")
print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")

updateGUI(statusLabel, typeLabel, doorLabel, normalButton, goldenButton, diamondButton, door1Button, door2Button, door3Button)
